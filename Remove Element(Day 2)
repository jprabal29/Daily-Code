class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        // vector<int> v;
        int i=0;
        for(int j=0;j<nums.size();j++){
            if(nums[j]!=val){
                // v.push_back(nums[i]);
                nums[i]=nums[j];
                i++;
            }
        }
       return i;
    }
};
